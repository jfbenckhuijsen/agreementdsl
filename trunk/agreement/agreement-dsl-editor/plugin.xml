<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.atosorigin.nl.dsl.agreement.editor.AgreementEditor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            extensions="plan"
            icon="icons/file.gif"
            id="com.atosorigin.nl.dsl.agreement.editor.AgreementEditor"
            name="agreement Editor">
      </editor>
   </extension>

   <extension
         point="org.eclipse.ui.newWizards">

		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="com.atosorigin.nl.dsl.agreement.wizards.NewAgreementProjectWizard"
			icon="icons/default.gif"
			id="com.atosorigin.nl.dsl.agreement.wizards.NewAgreementProjectWizard"
			name="Agreement Project"
			project="true">
		</wizard>

		<wizard
			category="org.openarchitectureware.xtext.wizards"
			class="com.atosorigin.nl.dsl.agreement.wizards.NewAgreementFileWizard"
			icon="icons/file.gif"
			id="com.atosorigin.nl.dsl.agreement.wizards.NewAgreementFileWizard"
			name="Agreement File">
		</wizard>

	</extension>
</plugin>
